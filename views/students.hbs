<ul id="students" class="m-0 p-0">
    {{#if students}}
        {{#each students}}
            <li class="m-1">
                    <form id="{{dataValues.id}}" class="outputForm d-flex row">
                        <input type="hidden" type="text" name="id" value="{{dataValues.id}}" placeholder="Identifier">
                        <div class="col-3">
                            <input name="lastName" value="{{dataValues.lastName}}" placeholder="lastname" class="inputActive input text-white color-passive" disabled="disabled">            
                            </input>
                        </div>
                        <div class="col-3">
                            <input name="firstName" value="{{dataValues.firstName}}" placeholder="firstname" class="inputActive input text-white color-passive" disabled="disabled">            
                            </input>
                        </div>
                        <div class="col-2">
                            <input name="groupId" value="{{dataValues.groupId}}" placeholder="group Id" class="inputActive input text-white color-passive" disabled="disabled">            
                            </input>
                        </div>
                        <div class="col-2">
                            <button id="{{dataValues.id}}" type="button"  class="buttonEdit button btn-danger">&#9998;</button>	
                        </div>
                        <div class="col-2 hide">
                            <button type="submit" class="buttonSave button btn-success">&#10004;</button>
                        </div>
                        <div class="col-2">
                            <button id="{{dataValues.id}}" type="submit"  class="buttonDel button btn-danger ">&#10006;</button>
                        </div>
                    </form>
            </li>
        {{/each}}
    {{/if}}
    <li id="inputLi" class="m-1">
        <button type="button" class="buttonAdd button btn-success ">&#10010;</button>
    </li> 
</ul>
